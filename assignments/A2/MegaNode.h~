// MegaNode.h

#ifndef MEGANODE_H
#define MEGANODE_H

#include <iostream>
#include <cmath>
#include <string>

using namespace std;

struct MegaNode
{
	string name;
	int dimension;
	double *vals;
	MegaNode *left, *right;
	
	MegaNode() : name(nullptr), dimension(0), vals(0), left(nullptr), right(nullptr)
	{}
};

#endif 
